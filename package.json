{
	"name": "block-editor-governance",
	"version": "1.0.0",
	"description": "This is a plugin for integrating governance into the block editor",
	"author": "VIP Bistro",
	"main": "build/index.js",
	"devDependencies": {
		"@automattic/eslint-plugin-wpvip": "0.5.4",
		"@wordpress/block-editor": "^12.1.0",
		"@wordpress/data": "^9.3.0",
		"@wordpress/hooks": "^3.33.0",
		"@wordpress/notices": "^4.1.0",
		"@wordpress/scripts": "^26.4.0",
		"eslint": "8.37.0",
		"husky": "8.0.3",
		"lint-staged": "^13.0.0",
		"lodash": "^4.17.21",
		"phplint": "2.0.5",
		"prettier": "npm:wp-prettier@2.8.5"
	},
	"scripts": {
		"lint:js": "eslint . --ext .js",
		"build": "wp-scripts build ./src/js/index.js",
		"phplint": "npx phplint '**/*.php' '!vendor/**' '!node_modules/**' > /dev/null",
		"phpcs": "vendor/bin/phpcs --cache",
		"phpcs:fix": "vendor/bin/phpcbf",
		"lint": "npm run phplint && npm run phpcs && npm run lint:js",
		"start": "wp-scripts start ./src/js/index.js",
		"prepare": "husky install"
	}
}